{
  "name": "gspro-slx-proxy",
  "description": "Proxy communication between SLX and GSPro",
  "version": "1.6.0",
  "license": "MIT",
  "author": {
    "email": "ken.j.davidson@live.ca",
    "name": "Ken Davidson",
    "url": "https://github.com/kenjdavidson"
  },
  "main": "dist-main/index.js",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "lint": "eslint . --ext js,ts,tsx",
    "package:publish": "electron-builder --config electron-builder.config.js --publish always",
    "package": "electron-builder --config electron-builder.config.js",
    "package:win": "electron-builder --config electron-builder.config.js --win",
    "package:win:publish": "electron-builder --config electron-builder.config.js --win",
    "prettier:check": "prettier --check -- src/**/*.{ts,tsx}",
    "prettier:write": "prettier --write -- src/**/*.{ts,tsx}",
    "testnode:coverage": "vitest run --coverage --config=vitest.config.ts",
    "testnode:watch": "vitest --config=vitest.config.ts",
    "testnode": "vitest run --config=vitest.config.ts",
    "testui:coverage": "vitest run --coverage --config=vitestui.config.ts",
    "testui:watch": "vitest  --config=vitestui.config.ts",
    "testui": "vitest run  --config=vitestui.config.ts",
    "test": "npm run testnode && npm run testui",
    "typecheck": "npm run typecheck:main && npm run typecheck:preload && npm run typecheck:renderer",
    "typecheck:main": "tsc -p src/main/tsconfig.json --noEmit",
    "typecheck:preload": "tsc -p src/preload/tsconfig.json --noEmit",
    "typecheck:renderer": "tsc -p src/renderer/tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@gr2m/net-interceptor": "^1.0.0",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "@vitejs/plugin-react": "^4.1.0",
    "@vitest/coverage-v8": "^1.1.0",
    "electron": "^27.1.2",
    "electron-builder": "^24.9.1",
    "eslint": "^8.49.0",
    "jsdom": "^23.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.12",
    "vite-plugin-electron": "^0.14.1",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.41.0",
    "@fluentui/react-icons": "^2.0.223",
    "@stdlib/string": "^0.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.16.0",
    "react-router-dom": "^6.16.0"
  }
}
